                  </Button>
                  <Button fx:id="PickUpButton" layoutX="494.0" layoutY="190.0" mnemonicParsing="false" onAction="#pressPickUpButton" text="Pick Up" />
                  <Button fx:id="PutDownButton" layoutX="554.0" layoutY="190.0" mnemonicParsing="false" onAction="#pressButton" text="Put Down" />
