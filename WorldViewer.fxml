                              <Stop color="#2e8615" />
                              <Stop color="#d6ffa4" offset="1.0" />
                           </stops>
                        </RadialGradient>
                     </fill>
                  </Rectangle>
                  <Rectangle arcHeight="5.0" arcWidth="5.0" height="62.0" layoutX="501.0" layoutY="326.0" stroke="BLACK" strokeType="INSIDE" width="125.0">
                     <fill>
                        <LinearGradient endX="1.0" endY="1.0" startX="0.18095238095238095">
                           <stops>
                              <Stop color="#4d941d" />
                              <Stop color="#66e444" offset="1.0" />
                           </stops>
                        </LinearGradient>
                     </fill>
                  </Rectangle>
                  <ImageView fx:id="imageView" fitHeight="372.0" fitWidth="450.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true" />
